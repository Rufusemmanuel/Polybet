{
  "name": "polypicks",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "vercel-build": "node -e \"console.log('DIRECT_URL set:', Boolean(process.env.DIRECT_URL))\" && prisma generate && cross-env PRISMA_MIGRATE_ADVISORY_LOCK_TIMEOUT=120000 prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "test:category": "tsc -p tsconfig.category-test.json && node .tmp-category-test/scripts/category-harness.js",
    "test:order-payload": "node scripts/dev-test-order-payload.mjs",
    "test:approvals": "tsx scripts/test-approvals.ts",
    "test:buy-order": "tsx scripts/test-buy-order.ts",
    "test:proxy-wallet": "tsx scripts/test-proxy-wallet.ts",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy"
  },
  "dependencies": {
    "@polymarket/builder-relayer-client": "^0.0.8",
    "@polymarket/builder-signing-sdk": "^0.0.8",
    "@polymarket/clob-client": "^5.1.3",
    "@prisma/client": "5.20.0",
    "@react-pdf/renderer": "^4.3.2",
    "@tanstack/react-query": "5.66.6",
    "@tanstack/react-query-devtools": "5.66.6",
    "axios": "1.7.9",
    "bcryptjs": "2.4.3",
    "classnames": "2.5.1",
    "date-fns": "3.6.0",
    "html-to-image": "^1.11.13",
    "iron-session": "^8.0.4",
    "jspdf": "^3.0.4",
    "next": "14.2.7",
    "pg": "8.13.1",
    "prisma": "5.20.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "viem": "^2.43.5",
    "ws": "8.18.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/node": "22.9.4",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.20",
    "cross-env": "^10.1.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.7",
    "postcss": "8.4.49",
    "server-only": "0.0.1",
    "tailwindcss": "3.4.13",
    "tsx": "^4.19.2",
    "typescript": "5.4.5"
  }
}
